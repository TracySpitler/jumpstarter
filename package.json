{
  "name": "mern-jumpstart",
  "version": "1.0.0",
  "description": "A jumpstart on the MERN stack.",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node bin/www",
    "server": "nodemon bin/www",
    "test": "echo \"Error: no test specified\" && exit 1",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TracySpitler/jumpstarter.git"
  },
  "author": "TSpitler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TracySpitler/jumpstarter/issues"
  },
  "homepage": "https://github.com/TracySpitler/jumpstarter#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^4.1.2",
    "dotenv": "^8.1.0",
    "express": "^4.17.3",
    "mongoose": ">=5.7.5",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "nodemon": "^1.19.1"
  }
}
